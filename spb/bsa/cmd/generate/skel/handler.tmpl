package handler

import (
	service "spb/bsa/internal/{{.PathName}}/service"
)

type Handler struct {
	service *service.Service
}

// @author: LoanTT
// @function: NewHandler
// @description: Create a new {{.ModuleName}} handler
// @param: {{.ModuleName}} serv
// @return: fiber.Handler
func NewHandler(serv *service.Service) *Handler {
	return &Handler{
		service: serv,
	}
}
