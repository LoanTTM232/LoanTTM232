package handler

import (
	service "spb/bsa/internal/{{.ModuleName}}/service"
)

type Handler struct {
	service *service.Service
}

// @author: LoanTT
// @function: NewHandler
// @description: Create a new {{.ModuleName}} handler
// @param: {{.ModuleName}} service
// @return: fiber.Handler
func NewHandler(service *service.Service) *Handler {
	return &Handler{
		service: service,
	}
}
